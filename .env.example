# Daytona API Configuration
DAYTONA_API_KEY=your_daytona_api_key_here
DAYTONA_API_URL=https://app.daytona.io/api

# Optional: Additional API keys for sandbox environments
ANTHROPIC_API_KEY=your_anthropic_key_if_needed
GEMINI_API_KEY=your_gemini_key_if_needed
GOOGLE_API_KEY=your_google_key_if_needed

# GitHub Configuration (Required for PR creation)
GITHUB_TOKEN=your_github_personal_access_token
GITHUB_USERNAME=your_github_username
GITHUB_EMAIL=your_github_email@example.com

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
DEBUG=false
REQUIRE_AUTH=false
API_KEY=your_api_key_here

# Resource Limits
MAX_SANDBOX_DURATION=600
MAX_FILE_SIZE=1048576
MAX_FILES_PER_PR=50

# Rate Limiting
RATE_LIMIT_REQUESTS=10
RATE_LIMIT_WINDOW=3600

# Observability
ENABLE_TELEMETRY=false
OTEL_ENDPOINT=